    "husky": "^9.1.7",
    "prettier": "^3.3.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "storybook": "^8.4.5",
    "types-node": "^1.3.0",
    "typescript": "~5.6.2",
    "typescript-eslint": "^8.11.0",
    "vite": "^5.4.10"
  },
  "peerDependencies": {
    "date-fns": "^4.1.0",
    "react": "^18.3.1"
  },
  "dependencies": {
    "classnames": "^2.5.1",
